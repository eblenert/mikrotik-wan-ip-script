:global currentIP;
:global url;
:local newIPString;
:local ipAddressParts;
:local newIP;
:local result;

:set newIPString [/ip address get [find interface="rds"] address];

:set newIP [:pick $newIPString 0 [:find $newIPString "/"]];

:if ($newIP != currentIP) do={
  :log info "IP address $currentIP changed to $newIP";
  :set currentIP $newIP;
  :set result [/tool fetch url="$url"];
}
